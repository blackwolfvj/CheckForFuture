{
      "manifest_version": 2,
      "name": "This Is Correct",
      "version": "0.1",
      "permissions": [ "storage" ],
      "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["jquery-3.4.0.min.js", "content.js"]
        }
      ],
      "browser_action": {
            "default_popup": "popup.html"
          }
    }